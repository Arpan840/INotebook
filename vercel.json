{
    "version": 2,
    "builds": [
      { "src": "./index.js", "use": "@vercel/node" }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/"
      },
      {
        "src": "/api/auth",
        "dest": "./Routes/auth.js"
      },
      {
        "src": "/api/auth",
        "dest": "./Routes/notes.js"
      }
    ]
  }
  